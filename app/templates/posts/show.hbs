<div class="ph5-ns">
{{#liquid-bind model use="toRight"}}
  <div class="center square ba bw1 b--light-gray flex items-center">
    <img class="w-100" src="{{model.source}}">
  </div>
  <p class="mt1 mb2 f6 mid-gray">Credits: {{model.credits}}</p>
{{/liquid-bind}}
  <!--
  <p>{{model.title}}</p>
  <p>{{model.description}}</p>
  -->

  {{posts/vote-buttons 
    model=model
    voteup=(action "voteup")
    votedown=(action "votedown")
  }}
</div>
